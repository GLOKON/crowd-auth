{
    "name": "glokon/laravel-crowd-auth",
    "description": "A simple way to implement Atlassian Crowd Authentication into your application.",
    "keywords": [
        "auth", "crowd", "authentication", "atlassian", "laravel"
    ],
    "homepage": "https://github.com/GLOKON/laravel-crowd-auth",
    "authors": [
        {
            "name": "Daniel McAssey",
            "email": "hello@glokon.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "*",
        "illuminate/auth": "4.2.*"
    },
    "require-dev": {
        "orchestra/testbench": "2.2.*",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.7.6"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "GLOKON\\CrowdAuth\\": "src/GLOKON/CrowdAuth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GLOKON\\Tests\\CrowdAuth\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "stable"
}