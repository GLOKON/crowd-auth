{
    "name": "glokon/crowd-auth",
    "description": "A simple way to implement Atlassian Crowd Authentication into your application.",
    "keywords": [
        "auth", "crowd", "authentication", "atlassian", "laravel"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/GLOKON/crowd-auth/issues",
        "source": "https://github.com/GLOKON/crowd-auth"
    },
    "homepage": "https://github.com/GLOKON/crowd-auth",
    "authors": [
        {
            "name": "Daniel McAssey",
            "email": "hello@glokon.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "*",
        "illuminate/auth": "4.2.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models",
            "src/lang"
        ],
        "psr-4": {
            "GLOKON\\CrowdAuth\\": "src/GLOKON/CrowdAuth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GLOKON\\Tests\\CrowdAuth\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    },
    "minimum-stability": "stable"
}